{
    "swagger": "2.0",
    "info": {
        "description": "Admin can add, update, delete and search Products into DB and User can fetch products",
        "title": "Swagger Products Service API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Sai Kumar Basaveswara",
            "email": "swiggyb2030@datascience.manipal.edu"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:3000",
    "paths": {
        "/": {
            "get": {
                "description": "This request will return 200 OK if server is up..",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Health"
                ],
                "summary": "To check if the service is running or not.",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "/products": {
            "get": {
                "security": [
                    {
                        "Bearer Token": []
                    }
                ],
                "description": "Fetches and displays all the products",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Product Service"
                ],
                "summary": "Fetches and displays all the products",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "number"
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "Bearer Token": []
                    }
                ],
                "description": "Adds Product to the DB",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Product Service"
                ],
                "summary": "Adds Product to the DB",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "/products/:productId": {
            "put": {
                "security": [
                    {
                        "Bearer Token": []
                    }
                ],
                "description": "Updates the Product details in DB",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Product Service"
                ],
                "summary": "Updates the Product details in DB",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Product"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "number"
                        }
                    }
                }
            },
            "delete": {
                "security": [
                    {
                        "Bearer Token": []
                    }
                ],
                "description": "Fetches and displays all the products",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Product Service"
                ],
                "summary": "Fetches and displays all the products",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "/products/search/:search": {
            "get": {
                "security": [
                    {
                        "Bearer Token": []
                    }
                ],
                "description": "Searches for Products in DB with given search term and returns list of Products",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Product Service"
                ],
                "summary": "Searches for Products in DB with given search term and returns list of Products",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Product"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "number"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Product": {
            "type": "object",
            "properties": {
                "can_place_order": {
                    "type": "boolean"
                },
                "height": {
                    "type": "number"
                },
                "id": {
                    "type": "string"
                },
                "image": {
                    "type": "string"
                },
                "length": {
                    "type": "number"
                },
                "manufacturer_id": {
                    "type": "integer"
                },
                "max_quantity": {
                    "type": "integer"
                },
                "minimum": {
                    "type": "integer"
                },
                "model": {
                    "type": "string"
                },
                "points": {
                    "type": "integer"
                },
                "price": {
                    "type": "number"
                },
                "product_category": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "product_description": {
                    "$ref": "#/definitions/models.ProductDescriptionModel"
                },
                "product_related": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "product_seo_url": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.ProductSeoUrlModel"
                    }
                },
                "quantity": {
                    "type": "integer"
                },
                "rewards": {
                    "type": "integer"
                },
                "sku": {
                    "type": "string"
                },
                "weight": {
                    "type": "number"
                },
                "width": {
                    "type": "number"
                }
            }
        },
        "models.ProductDescriptionModel": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string"
                },
                "language_id": {
                    "type": "integer"
                },
                "meta_description": {
                    "type": "string"
                },
                "meta_keyword": {
                    "type": "string"
                },
                "meta_title": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "models.ProductSeoUrlModel": {
            "type": "object",
            "properties": {
                "keyword": {
                    "type": "string"
                },
                "language_id": {
                    "type": "integer"
                },
                "store_id": {
                    "type": "integer"
                }
            }
        }
    },
    "securityDefinitions": {
        "ApiKeyAuth": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}